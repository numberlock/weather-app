{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-console": "off",
    "quotes": ["error", "double"],
    "strict": "off",
    "func-names": "off",
    "comma-dangle": "off"
  }
}
